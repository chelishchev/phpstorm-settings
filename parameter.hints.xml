<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="PHP">
        <added pattern="\Bitrix\Main\Loader.includeModule(moduleName)" />
        <added pattern="\Bitrix\Main\DB\Connection.query(sql)" />
        <added pattern="\sleep(seconds)" />
        <added pattern="\Bitrix\Main\Localization\Loc.getMessage(code,replace,language)" />
        <added pattern="\error_reporting(level)" />
        <added pattern="\ini_set(varname,newvalue)" />
        <added pattern="\var_dump(expression,_)" />
        <added pattern="\Bitrix\Disk\Driver.getStorageByCommonId(commonId)" />
        <added pattern="\Bitrix\Main\DB\Connection.queryExecute(sql,binds)" />
        <added pattern="\Bitrix\Main\DB\Connection.queryScalar(sql,binds)" />
        <added pattern="\Bitrix\Main\EventManager.addEventHandler(fromModuleId,eventType,callback,includeFile,sort)" />
        <added pattern="\Bitrix\Main\Event.getParameter(key)" />
        <added pattern="\app\models\LogOperations.mark(name,data)" />
        <added pattern="\Bitrix\Main\HttpRequest.getPost(name)" />
        <added pattern="\Bitrix\Main\Entity\Query.where(filter)" />
        <added pattern="\Bitrix\Main\Entity\Query.whereNull(column)" />
        <added pattern="\Bitrix\Main\Entity\Query.whereColumn(filter)" />
        <added pattern="\Bitrix\Main\Entity\Query.whereIn(column,values)" />
        <added pattern="\Bitrix\Main\Entity\Query.whereNotNull(column)" />
        <added pattern="\Bitrix\Main\Entity\Query.whereLike(column,value)" />
        <added pattern="\Bitrix\Main\Entity\Query.addSelect(definition,alias)" />
        <added pattern="\Bitrix\Main\Entity\Query.addOrder(definition,order)" />
        <added pattern="\Bitrix\Main\Entity\Query.addUnionOrder(definition,order)" />
        <added pattern="\Bitrix\Main\Entity\Query.setUnionLimit(limit)" />
      </blacklist>
    </blacklists>
  </component>
</application>
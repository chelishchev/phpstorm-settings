<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'http://one.ivandivan/bitrix/services/main/ajax.php'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'http://one.ivandivan/bitrix/services/main/ajax.php'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run to Cursor">
        <Item>
          <IdeaActionID>RunToCursor</IdeaActionID>
          <ShortCut>'Alt+F9'</ShortCut>
          <Description>Run to Cursor</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Annotate">
        <Item>
          <IdeaActionID>Annotate</IdeaActionID>
          <Description>Annotate</Description>
        </Item>
      </Statistic>
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Button4 Click' or 'Alt+Shift+Left'</ShortCut>
          <Description>Back</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste from X clipboard">
        <Item>
          <IdeaActionID>EditorPasteFromX11</IdeaActionID>
          <ShortCut>'Button2 Click'</ShortCut>
          <Description>Paste from X clipboard</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>